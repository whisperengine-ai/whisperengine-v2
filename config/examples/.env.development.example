# =======================================================
# WhisperEngine Development Configuration
# Full development features with debugging and hot-reload
# =======================================================

# =======================================================
# DISCORD CONFIGURATION
# =======================================================
DISCORD_BOT_TOKEN=your_discord_bot_token_here
DISCORD_BOT_NAME=WhisperEngine-Dev
ADMIN_USER_IDS=your_discord_user_id_here
DISCORD_MESSAGE_TRACE=true

# =======================================================
# DEVELOPMENT ENVIRONMENT
# =======================================================
ENV_MODE=development
ENVIRONMENT=development
DEBUG_MODE=true
DEV_MODE=true
CONTAINER_MODE=false

# =======================================================
# CHARACTER CONFIGURATION
# =======================================================
CDL_DEFAULT_CHARACTER=characters/examples/elena-rodriguez.json

# Multi-Entity Character System Features  
ENABLE_MULTI_ENTITY_RELATIONSHIPS=true
ENABLE_CHARACTER_CREATION=true
ENABLE_RELATIONSHIP_EVOLUTION=true
ENABLE_AI_FACILITATED_INTRODUCTIONS=true
ENABLE_CROSS_CHARACTER_AWARENESS=true
ENABLE_CHARACTER_SIMILARITY_MATCHING=true
ENABLE_SOCIAL_NETWORK_ANALYSIS=true

# Character System Limits
MAX_CHARACTERS_PER_USER=10
MAX_CHARACTER_NAME_LENGTH=50
MAX_CHARACTER_BACKGROUND_LENGTH=1000

# Roleplay System Configuration
ENABLE_ROLEPLAY_COMMANDS=true
ROLEPLAY_MEMORY_ISOLATION=true
ROLEPLAY_AUTO_CLEAR_ON_SWITCH=true
ROLEPLAY_SESSION_TIMEOUT_MINUTES=120

# =======================================================
# LLM CONFIGURATION (LOCAL DEVELOPMENT)
# =======================================================
LLM_CHAT_API_URL=http://localhost:1234/v1
LLM_CHAT_API_KEY=not-needed
LLM_CHAT_MODEL=local-model
LLM_TEMPERATURE=0.7
LLM_MAX_TOKENS_CHAT=4096
LLM_REQUEST_TIMEOUT=90

# =======================================================
# ADVANCED LOGGING (DEVELOPMENT)
# =======================================================
LOG_LEVEL=DEBUG
CONSOLE_LOG_LEVEL=DEBUG
PYTHONUNBUFFERED=1
FORCE_READABLE_LOGS=true

# Performance monitoring
ENABLE_PERFORMANCE_MONITORING=true
PERFORMANCE_LOG_LEVEL=debug

# =======================================================
# MEMORY SYSTEM (VECTOR-NATIVE)
# =======================================================
MEMORY_SYSTEM_TYPE=vector
VECTOR_QDRANT_HOST=localhost
VECTOR_QDRANT_PORT=6333
VECTOR_QDRANT_GRPC_PORT=6334
VECTOR_QDRANT_COLLECTION=whisperengine_memory
VECTOR_EMBEDDING_MODEL=snowflake/snowflake-arctic-embed-xs

# Multi-Bot Memory Configuration (Development)
ENABLE_MULTI_BOT_QUERIES=true
MULTI_BOT_MEMORY_ISOLATION=true
WHISPERENGINE_MODE=single_bot

# Memory debugging
ENABLE_MEMORY_MONITORING=true
MEMORY_OPTIMIZATION_LEVEL=debug

# Vector Memory Enhancements (Based on External Claude Recommendations)
ENABLE_MEMORY_DECAY_SYSTEM=true
ENABLE_MEMORY_SIGNIFICANCE_SCORING=true
ENABLE_EMOTIONAL_TRAJECTORY_TRACKING=true
ENABLE_MULTI_QUERY_RETRIEVAL=true

# Memory Decay Configuration
MEMORY_DECAY_HALF_LIFE_DAYS=30
MEMORY_DECAY_MINIMUM_SCORE=0.1
MEMORY_DECAY_PROTECTION_THRESHOLD=0.8

# Emotional Intelligence Enhancement
EMOTIONAL_TRAJECTORY_WINDOW=5
EMOTIONAL_VELOCITY_THRESHOLD=0.3
EMOTIONAL_STABILITY_WINDOW=10

# Multi-Query Retrieval Configuration
ENABLE_QUERY_VARIATIONS=true
MAX_QUERY_VARIATIONS=3
QUERY_VARIATION_WEIGHT=0.8

# =======================================================
# PHASE 1, 2 & 3 MEMORY FEATURES (DEVELOPMENT)
# =======================================================
# Phase Features Control
ENABLE_PHASE1_ENHANCED_MEMORY=true
ENABLE_PHASE2_THREE_TIER_MEMORY=true
ENABLE_PHASE3_MEMORY_NETWORKS=true
ENABLE_PHASE4_INTELLIGENCE=true
DISABLE_PHASE2_EMOTION=false

# Phase 1: Enhanced Memory Features
PHASE1_EMOTIONAL_DETECTION_ENABLED=true
PHASE1_EMOTIONAL_TRAJECTORY_ENABLED=true
PHASE1_SIGNIFICANCE_SCORING_ENABLED=true

# Phase 2: Three-Tier Memory & Decay (Development Settings)
PHASE2_THREE_TIER_ENABLED=true
PHASE2_MEMORY_DECAY_ENABLED=true
PHASE2_MEMORY_DECAY_RATE=0.05
PHASE2_TIER_PROMOTE_TO_MEDIUM_SIGNIFICANCE=0.5
PHASE2_TIER_PROMOTE_TO_LONG_SIGNIFICANCE=0.7
PHASE2_MEMORY_DECAY_PROTECTION_THRESHOLD=0.7
EMOTION_GRAPH_SYNC_INTERVAL=180

# Phase 3: Context Awareness & Empathy Calibration
ENABLE_PHASE3_CONTEXT_DETECTION=true
ENABLE_PHASE3_EMPATHY_CALIBRATION=true
DISABLE_PHASE3_CONTEXT_DETECTION=false
DISABLE_PHASE3_EMPATHY_CALIBRATION=false

# Phase 3: Context Switch Detection Thresholds
PHASE3_TOPIC_SHIFT_THRESHOLD=0.4
PHASE3_EMOTIONAL_SHIFT_THRESHOLD=0.3
PHASE3_CONVERSATION_MODE_THRESHOLD=0.5
PHASE3_URGENCY_CHANGE_THRESHOLD=0.4
PHASE3_MAX_MEMORIES=50
PHASE3_ANALYSIS_TIMEOUT=60

# Phase 3: Empathy Calibration Parameters
PHASE3_EMPATHY_MIN_INTERACTIONS=3
PHASE3_EMPATHY_EFFECTIVENESS_THRESHOLD=0.6
PHASE3_EMPATHY_LEARNING_RATE=0.1
PHASE3_EMPATHY_CONFIDENCE_THRESHOLD=0.7

# =======================================================
# DATABASE CONFIGURATION (LOCAL)
# =======================================================
# PostgreSQL
POSTGRESQL_HOST=localhost
POSTGRESQL_PORT=5432
POSTGRESQL_DATABASE=whisper_engine_dev
POSTGRESQL_USERNAME=bot_user
POSTGRESQL_PASSWORD=dev_password_123

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
USE_REDIS_CACHE=true
REDIS_CACHE_TTL_MINUTES=5

# =======================================================
# AI INTELLIGENCE FEATURES (ALL ENABLED)
# =======================================================
ENABLE_EMOTIONAL_INTELLIGENCE=true
ENABLE_PHASE3_MEMORY=true
ENABLE_PHASE4_HUMAN_LIKE=true
ENABLE_PHASE4_THREAD_MANAGER=true
ENABLE_PHASE4_PROACTIVE_ENGAGEMENT=true

# Development AI features
AI_EMOTIONAL_RESONANCE=true
AI_ADAPTIVE_MODE=true
AI_PERSONALITY_ANALYSIS=true
ENABLE_DYNAMIC_PERSONALITY=true

# =======================================================
# PERFORMANCE OPTIMIZATION (DEVELOPMENT)
# =======================================================
ENABLE_PRODUCTION_OPTIMIZATION=true
ENABLE_PARALLEL_PROCESSING=true
AI_MEMORY_OPTIMIZATION=true
CACHE_STRATEGY=adaptive
ENABLE_BACKGROUND_PROCESSING=true

# Circuit Breaker Configuration (Development)
ENABLE_CIRCUIT_BREAKER=true
CIRCUIT_BREAKER_FAILURE_THRESHOLD=5
CIRCUIT_BREAKER_RECOVERY_TIMEOUT=30
CIRCUIT_BREAKER_HALF_OPEN_MAX_CALLS=3

# Performance Monitoring
ENABLE_PERFORMANCE_METRICS=true
MEMORY_PERFORMANCE_LOGGING=true
VECTOR_OPERATION_TIMEOUT=10

# =======================================================
# VOICE FEATURES (DEVELOPMENT)
# =======================================================
VOICE_SUPPORT_ENABLED=true
VOICE_RESPONSE_ENABLED=true
VOICE_AUTO_JOIN=false
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# =======================================================
# BACKUP & MAINTENANCE
# =======================================================
AUTO_BACKUP_ENABLED=true
AUTO_BACKUP_INTERVAL_HOURS=6
BACKUP_RETENTION_COUNT=10

# =======================================================
# DEVELOPMENT DEBUGGING
# =======================================================
# Multi-entity debugging
MULTI_ENTITY_DEBUG=true
LOG_RELATIONSHIP_CHANGES=true
LOG_INTERACTION_DETAILS=true

# Memory debugging
SEMANTIC_CLUSTERING_TIMEOUT=60
EMBEDDING_MAX_RETRIES=5

# Job scheduler debugging
JOB_SCHEDULER_ENABLED=true
JOB_SCHEDULER_CHECK_INTERVAL_SECONDS=10

# =======================================================
# HEALTH MONITORING
# =======================================================
HEALTH_CHECK_PORT=9090
HEALTH_CHECK_HOST=0.0.0.0

# =======================================================
# ADVANCED AI FEATURES (DEVELOPMENT)
# =======================================================
# Enable all experimental features for development
EXPERIMENTAL_FEATURES_ENABLED=true
BETA_MEMORY_FEATURES=true
BETA_CONVERSATION_FEATURES=true